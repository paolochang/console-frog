{
  "name": "console-frog",
  "displayName": "console.frog",
  "description": "Print console üê∏",
  "version": "0.0.1",
  "publisher": "paolochang",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:frog.consoleYellow",
    "onCommand:frog.consoleIvory",
    "onCommand:frog.consoleOrange",
    "onCommand:frog.consolePrint",
    "onCommand:frog.consoleKhaki",
    "onCommand:frog.consoleLime"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "frog.consoleYellow",
        "key": "ctrl+alt+y",
        "mac": "ctrl+alt+y"
      },
      {
        "command": "frog.consoleIvory",
        "key": "ctrl+alt+i",
        "mac": "ctrl+alt+i"
      },
      {
        "command": "frog.consoleOrange",
        "key": "ctrl+alt+o",
        "mac": "ctrl+alt+o"
      },
      {
        "command": "frog.consolePrint",
        "key": "ctrl+alt+p",
        "mac": "ctrl+alt+p"
      },
      {
        "command": "frog.consoleKhaki",
        "key": "ctrl+alt+k",
        "mac": "ctrl+alt+k"
      },
      {
        "command": "frog.consoleLime",
        "key": "ctrl+alt+l",
        "mac": "ctrl+alt+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
